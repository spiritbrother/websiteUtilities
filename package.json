{
    "name": "jQuery-website",
    "version": "1.0.0",
    "description": "extend your web app",
    "keywords": ["jquery", "website"],
    "license": "CC-BY-3.0",
    "files": ["index.js", "index.less", "font"],
    "repository": {
        "type": "git", "url": "https://github.com/thaibault/jQuery-website"
    },
    "homepage": "http://torben.website/jQuery-website",
    "main": "index.compiled.js",
    "webOptimizer": {
        "path": {"asset": {"font": "./font.compiled/"}},
        "module": {"aliases": {
            "bootstrap.less$": "bootstrap/less/bootstrap",
            "jQuery-scrollTo$": "jquery.scrollto/jquery.scrollTo",
            "jQuery-spin$": "spin.js/jquery.spin",
            "genericStyle.less$": "genericStyle/index.less"
        }}
    },
    "scripts": {
        "build": "webOptimizer build",
        "buildDLL": "webOptimizer buildDLL",
        "clear": "webOptimizer clear",
        "document": "webOptimizer document",
        "export": "webOptimizer build '{exportFormat:\"var\"}'",
        "lint": "webOptimizer typeCheck && webOptimizer lint",
        "test": "webOptimizer test",
        "postinstall": "webOptimizer build",
        "preinstall": "webOptimizer preinstall",
        "testInBrowser": "webOptimizer testInBrowser",
        "watch": "webOptimizer build --watch",
        "watchDLL": "webOptimizer buildDLL --watch"
    },
    "dependencies": {
        "webOptimizer": "git+ssh://git@github.com/thaibault/webOptimizer.git",
        "jQuery-tools": "git+ssh://git@github.com/thaibault/jQuery-tools.git",
        "jQuery-lang": "git+ssh://git@github.com/thaibault/jQuery-lang.git",
        "spin.js": "2.3.2",
        "jquery.scrollto": "2.1.2",
        "genericStyle": "git+ssh://git@github.com/thaibault/genericStyle.git",
        "bootstrap": "^3.3.6"
    },
    "devDependencies": {
        "jsdom": ">=9.4.0",
        "qunitjs": ">=2.0.0",
        "qunit-cli": ">=0.2.0*",
        "source-map-support": ">=0.4.0"
    }
}
